[color]
    ui = true

[core]
    autocrlf = false
	eof = lf

[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
    
    s = status
    c = commit
    ca = commit -a
    co = checkout
    dc = diff --cached
    b = branch
    ba = branch -av
    p = pull
    pm = push origin master
    d = diff

[push]
	default = matching
[pull]
    ff = only
